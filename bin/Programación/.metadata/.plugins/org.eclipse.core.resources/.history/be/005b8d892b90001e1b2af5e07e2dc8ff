package repasoT2;

public class Ej7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int suma=0;
		
		for (int i = 1; i <=10; i++) {
			int numero = i*3;
			suma +=numero;
			if(i == 10) {
				System.out.print(numero + "=" + suma);
			}else {
				System.out.print(numero + "+");
			}
		}
	}

}
